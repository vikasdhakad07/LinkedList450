Method 1:Iterative Method


class Solution
{
    //Function to reverse a linked list.
    Node reverseList(Node head)
    {
 Node prev=null;
        Node temp=null;
        Node curr=node;
        while(curr!=null){
            temp=curr.next;
            curr.next=prev;
            prev=curr;
            curr=temp;
        }
        return prev;
        }
        }
